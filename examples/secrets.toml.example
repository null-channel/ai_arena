# Example secrets configuration file for ai_arena
# Copy this file to ~/.config/ai_arena/secrets.toml and fill in your API keys
# Make sure to set proper permissions: chmod 600 ~/.config/ai_arena/secrets.toml

[secrets.openai.default]
api_key = "sk-your-openai-api-key-here"

[secrets.openai.work]
api_key = "sk-your-work-openai-api-key-here"

[secrets.anthropic.default]
api_key = "sk-ant-your-anthropic-api-key-here"

[secrets.anthropic.personal]
api_key = "sk-ant-your-personal-anthropic-api-key-here"

[secrets.ollama.local]
base_url = "http://localhost:11434"

[secrets.ollama.remote]
base_url = "http://192.168.1.100:11434"

